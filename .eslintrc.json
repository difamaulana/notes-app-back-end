{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "eol-last": "off",
        "no-trailing-spaces": "off",
        "no-unused-vars": "off",
        "indent": "off",
        "padded-blocks": "off",
        "object-curly-newline":"off",
        "max-len": "off",
        "consistent-return":"off"
    }
}